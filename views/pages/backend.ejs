<%- include('../partials/head'); -%>

<div class="container">
    <div class="row p-3">
        <div class="col-md-4">
            <label class="">Password</label>
            <input id="pass" class="form-control" placeholder="pass" />
            <button class="btn btn-primary" id="pass-button">Submit</button>
        </div>
    </div>

    <div id="backend-wrapper">
        <div class="p-3">
            <div class="row mb-2">
                <div class="col">
                    <h1>Upload New</h1>
                </div>
            </div>
            <div class="row mb-2">
                <div class="col-md-8">
                    <label for="title">Title</label>
                    <input class="form-control" id="title" />
                </div>
            </div>
            <div class="row mb-2">
                <div class="col-md-4">
                    <label for="artist1-name">Artist 1 name</label>
                    <input class="form-control  mb-2" id="artist1-name" />
                    <label for="artist2-name">Artist 2 name</label>
                    <input class="form-control  mb-2" id="artist2-name" />
                </div>
                <div class="col-md-4">
                    <label for="artist1-link">Artist 1 link</label>
                    <input class="form-control mb-2" id="artist1-link" />
                    <label for="artist2-link"> Artist 2 link</label>
                    <input class="form-control mb-2" id="artist2-link" />
                </div>
            </div>
            <div class="row mb-2">
                <div class="col-md-8">
                    <label for="title">Img</label>
                    <input class="form-control-file" type="file" id="img" />
                </div>
            </div>
            <div class="row mb-2">
                <div class="col-md-8">
                    <label for="Audio">Audio</label>
                    <input class="form-control-file" type="file" id="audio" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-8 ">
                    <button id="save-new" class="btn btn-success float-right">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="/js/compressor.min.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<script src="/js/backend.js"></script>